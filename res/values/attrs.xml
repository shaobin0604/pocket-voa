<?xml version="1.0" encoding="utf-8"?>
<resources>
	<attr name="focusLineColor" format="color" />
	<attr name="otherLineColor" format="color" />
	<attr name="textSize" format="dimension" />
	<attr name="errorMessage" format="string" />
    
	<declare-styleable name="com.admob.android.ads.AdView">
		<attr name="testing" format="boolean" />
		<attr name="backgroundColor" format="color" />
		<attr name="textColor" format="color" />
		<attr name="keywords" format="string" />
		<attr name="refreshInterval" format="integer" />
		<attr name="isGoneWithoutAd" format="boolean" />
	</declare-styleable>
	
	<declare-styleable name="LyricView">
		<attr name="focusLineColor"/>
		<attr name="otherLineColor"/>
		<attr name="textSize"/>
		<attr name="errorMessage"/>
	</declare-styleable>
	
	<declare-styleable name="LyricView2">
		<attr name="focusLineColor"/>
		<attr name="otherLineColor"/>
		<attr name="textSize"/>
		<attr name="errorMessage"/>
	</declare-styleable>
	
	<declare-styleable name="TabBar">
        <attr name="tabTextColor" format="color"/>
        <attr name="tabTextSize" format="dimension"/>
        <attr name="tabWidth" format="dimension"/>
        <attr name="tabHeight" format="dimension"/>
        <attr name="tabSpacing" format="dimension"/>
        <attr name="tabButtonDrawable" format="reference"/>
        <attr name="tabBarDrawable" format="reference"/>
        <attr name="tabHorizontalPadding" format="dimension"/>
        <attr name="tabSeperatorDrawable" format="reference"/>
        <attr name="tabSeperatorHeight" format="dimension"/>
    </declare-styleable>
</resources>
